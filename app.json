{
  "expo": {
    "name": "Thyme to Dine",
    "slug": "thymetodine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/Stew.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/Stew.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/Stew.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/Stew.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ]
  }
}
